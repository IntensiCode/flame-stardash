{
	alias: true,
	alphaThreshold: 0,
	atlasExtension: .atlas,
	bleed: true,
	bleedIterations: 2,
	combineSubdirectories: true,
	debug: false,
	duplicatePadding: false,
	edgePadding: true,
	fast: false,
	filterMag: Nearest,
	filterMin: Nearest,
	flattenPaths: false,
	format: RGBA8888,
	grid: false,
	ignoreBlankImages: true,
	jpegQuality: 0.9,
	legacyOutput: true,
	limitMemory: true,
	maxHeight: 2048,
	maxWidth: 2048,
	minHeight: 16,
	minWidth: 16,
	outputFormat: png,
	paddingX: 2,
	paddingY: 2,
	pot: true,
	premultiplyAlpha: false,
	prettyPrint: true,
	rotation: false,
	scale: [ 1 ],
	scaleResampling: [ bicubic ],
	scaleSuffix: [ "" ],
	square: false,
	stripWhitespaceX: false,
	stripWhitespaceY: false,
	useIndexes: true,
	wrapX: ClampToEdge,
	wrapY: ClampToEdge,
}
